                    android:background="@color/tx"

                    >

                    <Button
                        android:id="@+id/apply"
                        android:layout_width="133dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/downloads"
                        android:gravity="center"
                        android:text="@string/down"
                        android:textColor="@color/ba" />

                    <TextView
                        android:id="@+id/download1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/broucher"
                        android:textColor="@android:color/white"
                        android:gravity="center"
                        android:background="@color/col"
                        android:layout_gravity="center"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="25dp"
                        android:textSize="20dp"
                        android:layout_marginBottom="20dp"
                        android:layout_marginLeft="25dp"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@color/tx"

                    >


                    <TextView
                        android:id="@+id/download2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/chavani"
                        android:textColor="@android:color/white"
                        android:gravity="center"
                        android:background="@color/col"
                        android:layout_gravity="center"
                        android:layout_marginRight="25dp"
                        android:layout_marginBottom="20dp"
                        android:textSize="20dp"
                        android:layout_marginLeft="25dp"/>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/provide"
                    android:layout_marginRight="25dp"
                    android:textSize="18dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginLeft="25dp"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="25dp"
                    android:layout_marginRight="25dp"
                    android:layout_marginBottom="20dp"

                    android:orientation="horizontal">

                    <EditText
                        android:id="@+id/first"
                        android:layout_width="230dp"
                        android:layout_height="37dp"
                        android:paddingLeft="10dp"
                        android:layout_gravity="center"
                        android:background="@drawable/txt"
                        android:hint="@string/id"
                        android:textSize="17dp" />

                    <Button
                        android:id="@+id/notify"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/col"
                        android:text="@string/me"
                        android:layout_gravity="center"
                        android:textColor="@android:color/white" />


                </LinearLayout>




                <Button
                    android:id="@+id/ss1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="awards"
                    android:layout_marginTop="20dp"
                    android:layout_gravity="center"/>


            </LinearLayout>

        </ScrollView>

        <com.google.android.material.bottomnavigation.BottomNavigationView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
